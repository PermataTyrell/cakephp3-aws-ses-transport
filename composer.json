{
    "name": "tyrellsys/cakephp3-aws-ses-transport",
    "description": "AwsSesTransport plugin for CakePHP3",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.0 <4.0.0",
        "aws/aws-sdk-php": "^3.19"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakePHP3AwsSesTransport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakePHP3AwsSesTransport\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
